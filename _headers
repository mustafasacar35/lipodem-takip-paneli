# PWA ve Manifest dosyaları için headers
/manifest.json
  Content-Type: application/manifest+json

/service-worker.js  
  Content-Type: application/javascript
  Service-Worker-Allowed: /

# Cache control
/*.html
  Cache-Control: no-cache

/*.js
  Cache-Control: public, max-age=31536000

/*.json
  Cache-Control: no-cache